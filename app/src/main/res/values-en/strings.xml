    <string name="l2_g2_text_2"> (он) сестра</string>
    <string name="l2_g2_text_3"> (они) тетя</string>
    <string name="l2_g2_text_4"> (она) дочь</string>
    <string name="l2_g2_text_5"> (мы) сын</string>
